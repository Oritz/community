.box_client{ padding:20px 20px 20px 190px; margin-left:20px;margin-top:20px;width:375px;height:171px;background-color: #b5ced2;background-image: url(<%= asset_path("img/www_cont_block4_client.png") %>);background-repeat: no-repeat;background-position: left bottom;}
