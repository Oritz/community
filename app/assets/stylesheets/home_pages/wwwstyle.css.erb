.box_forum{ padding:20px 20px 20px 190px; margin-top:20px; width:209px;height:171px;background-color: #b5ced2;background-image: url(<%= asset_path("img/www_cont_block4_forum.png") %>);background-repeat: no-repeat;background-position: left bottom;}
