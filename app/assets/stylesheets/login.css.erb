/* formbox-style */
.formbox-style{width:320px; overflow:hidden; padding:20px; border:1px solid #ccc;-webkit-border-radius: 3px;-moz-border-radius: 3px;border-radius: 3px;}
.formbox-style .item{height:65px;overflow:hidden;line-height:20px; font-size:12px;}
.formbox-style .item a:link,#formbox .item a:visited{text-decoration:underline;}
.formbox-style .label{width:320px;font-size:12px; line-height:20px; height:20px; color:#2c3e50; font-weight:bold;}
.formbox-style .span-150{width:150px;}

.formbox-style .text{width:306px;height:23px; line-height:23px;padding:0px 5px;border:1px solid #d4d4d4;font-size:12px;-webkit-border-radius: 3px;-moz-border-radius: 3px;border-radius: 3px;color:#666;}
.formbox-style .text-1{width:218px;}
.formbox-style .highlight1{border-color:#3498db;outline: 0 none;}
.formbox-style .highlight2{border-color:#e74c3c;outline: 0 none;}

.formbox-style .img img{height:26px;margin:0 5px;}
.formbox-style .succeed{background:url(images/pwdstrength.gif) no-repeat -105px 0;}


/* yellow_button */

.yellow_button{border:none;cursor:pointer;height:29px; padding:0px 20px 1px 20px; overflow:hidden; display:inline-block; text-align:center; font-size:14px; font-weight:800; color:#fff;

               background:#3498db;-webkit-border-radius: 3px;-moz-border-radius: 3px;border-radius: 3px;  transition: all 0.3s ease 0s; }
.yellow_button:hover{ background:#2980b9;}
#registsubmit.disabled{ background:#ccc;cursor:default!important;}

#formbox .pwdbg{background:#FFF8EB;}
#formbox .focus{color:#999;line-height:15px;*line-height:15px; height:15px;}
#formbox .null,#formbox .error{color:red;line-height:20px;*line-height:22px; height:20px;}
#formbox .checkbox{margin-top:6px;*margin-top:2px;}
#formbox #referrer{color:#999;font-size:12px;}
#formbox #protocol{margin:3px 5px 0 0;display:inline; cursor:pointer;}
#pwdstrength{color:#999;line-height:22px;padding-right:10px;}
#pwdstrength b{float:left;width:104px;height:13px;overflow:hidden;margin-top:5px;*margin-top:3px;}
.strengthA b{background:url(images/pwdstrength.gif) no-repeat 0 0;}
.strengthB b{background:url(images/pwdstrength.gif) no-repeat 0 -13px;}
.strengthC b{background:url(images/pwdstrength.gif) no-repeat 0 -26px;}




