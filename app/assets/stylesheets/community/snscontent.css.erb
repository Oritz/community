	#focus_Box {
	position:relative;
	width:170px;
	height:100px;
	margin:10px auto;
}
#focus_Box ul {
	position:relative;
	width:130px;
	height:100px;
	margin-left:20px;
}
#focus_Box li {
	z-index:0;
	position:absolute;
	width:70px;
	height:100px;
	top:0px;
	cursor:pointer;
	left:40px;
}
#focus_Box li img {
	width:100%;
	height:100%;
	vertical-align:top;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	border:1px solid #2c3e50;
}
#focus_Box .prev,#focus_Box .next {
	display:block;
	z-index:100;
	overflow:hidden;
	cursor:pointer;
	position:absolute;
	width:10px;
	height:16px;
	top:50px;
}
